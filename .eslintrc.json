{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "warn",
        "camelcase": "warn",
        "linebreak-style": "warn",
        "max-len": "warn"
    }
}
